<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <display-name>Hospice</display-name>
    <servlet>
        <servlet-name>Hospice</servlet-name>
        <servlet-class>Hospice</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>personalInfo</servlet-name>
        <servlet-class>personalInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SecondServlet</servlet-name>
        <servlet-class>SecondServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loadingPersonalData</servlet-name>
        <servlet-class>loadingPersonalData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Medication</servlet-name>
        <servlet-class>Medication</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>popupform</servlet-name>
        <servlet-class>popupform</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Homepage</servlet-name>
        <servlet-class>Homepage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Hospice</servlet-name>
        <url-pattern>/Hospice</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>login</welcome-file>
        <welcome-file>loginpage.jsp</welcome-file>
    </welcome-file-list>
    <servlet-mapping>
        <servlet-name>personalInfo</servlet-name>
        <url-pattern>/personalInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SecondServlet</servlet-name>
        <url-pattern>/SecondServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loadingPersonalData</servlet-name>
        <url-pattern>/loadingPersonalData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Medication</servlet-name>
        <url-pattern>/Medication</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>popupform</servlet-name>
        <url-pattern>/popupform</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Homepage</servlet-name>
        <url-pattern>/Homepage</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
</web-app>
